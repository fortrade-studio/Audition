<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_tab"
    app:startDestination="@id/profileTabFragment">

    <fragment
        android:id="@+id/profileTabFragment"
        android:name="com.atria.myapplication.ProfileTabFragment"
        android:label="fragment_profile_tab"
        tools:layout="@layout/fragment_profile_tab" />
    <fragment
        android:id="@+id/aboutFragment2"
        android:name="com.atria.myapplication.AboutFragment"
        android:label="fragment_about"
        tools:layout="@layout/fragment_about" />
    <fragment
        android:id="@+id/imagesFragment"
        android:name="com.atria.myapplication.ImagesFragment"
        android:label="fragment_images"
        tools:layout="@layout/fragment_images" />
    <fragment
        android:id="@+id/videoFragment"
        android:name="com.atria.myapplication.VideoFragment"
        android:label="fragment_video"
        tools:layout="@layout/fragment_video" >
        <action
            android:id="@+id/action_videoFragment_to_cameraFragment"
            app:destination="@id/cameraFragment" />
    </fragment>
    <fragment
        android:id="@+id/cameraFragment"
        android:name="com.atria.myapplication.CameraFragment"
        android:label="fragment_camera"
        tools:layout="@layout/fragment_camera" />
</navigation>